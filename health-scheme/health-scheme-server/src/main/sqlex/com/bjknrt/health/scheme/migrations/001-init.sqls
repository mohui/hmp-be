/* hs_other_scheme */
CREATE TABLE `hs_other_scheme`
(
    `kn_id`               bigint(20) unsigned NOT NULL COMMENT '主键',
    `kn_descriptions`     varchar(255)        NOT NULL COMMENT '其它建议',
