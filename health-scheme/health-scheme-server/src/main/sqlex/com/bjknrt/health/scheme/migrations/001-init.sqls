    PRIMARY KEY (`kn_id`)
) ENGINE = InnoDB
  DEFAULT CHARSET = utf8mb4
  COLLATE = utf8mb4_general_ci COMMENT ='其它方案表';

/* hs_sport_scheme */
CREATE TABLE `hs_sport_scheme`
(
    `kn_id`               bigint(20) unsigned NOT NULL COMMENT '主键',
    `kn_types`            varchar(100)        NOT NULL COMMENT '运动种类',
    `kn_formal`           varchar(100)        NOT NULL COMMENT '运动形式',
    `kn_frequency`        varchar(50)         NOT NULL COMMENT '运动频次(NO_RECOMMEND:不推荐；ONCE_A_WEEK:每周1次；TWICE_A_WEEK:每周两次；THREE_TIMES_A_WEEK:每周三次；FOUR_TIMES_A_WEEK:每周四次；FIVE_TIMES_A_WEEK:每周5次；SIX_TIMES_A_WEEK:每周6次；SEVEN_TIMES_A_WEEK:每周7次；NEUROMOTOR_MEDICAL_ADVICE:遵医嘱执行)',
    `kn_duration`         varchar(100)        NOT NULL COMMENT '运动持续时间',
    `kn_degree`           varchar(255)        NOT NULL COMMENT '运动强度',
    `kn_reason`           varchar(255) DEFAULT '' COMMENT '备注',
    `kn_health_scheme_id` bigint(20) unsigned NOT NULL COMMENT '健康方案ID',
    PRIMARY KEY (`kn_id`)
) ENGINE = InnoDB
  DEFAULT CHARSET = utf8mb4
  COLLATE = utf8mb4_general_ci COMMENT ='运动方案表';

/* hs_stage_report */
CREATE TABLE `hs_stage_report`
(
    `id`                               bigint(20) unsigned NOT NULL,
    `health_scheme_management_info_id` bigint(20) unsigned NOT NULL COMMENT '健康方案管理信息id',
    `patient_id`                       bigint(20) unsigned NOT NULL COMMENT '患者id',
    `patient_name`                     varchar(50)         NOT NULL COMMENT '患者姓名',
    `report_name`                      varchar(50)         NOT NULL COMMENT '报告名称',
    `report_score`                     decimal(5, 2)                DEFAULT NULL COMMENT '报告得分',
    `report_start_datetime`            datetime            NOT NULL COMMENT '报告开始时间',
    `report_end_datetime`              datetime            NOT NULL COMMENT '报告截止时间',
    `created_by`                       bigint(20) unsigned NOT NULL COMMENT '创建人',
    `created_at`                       datetime            NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT '创建时间',
    PRIMARY KEY (`id`)
) ENGINE = InnoDB
  DEFAULT CHARSET = utf8mb4
  COLLATE = utf8mb4_general_ci COMMENT ='阶段性报告';

/* hs_stage_statistics */
CREATE TABLE `hs_stage_statistics`
(
    `id`                                                bigint(20) unsigned NOT NULL,
    `stage_report_id`                                   bigint(20) unsigned NOT NULL COMMENT '阶段性报告id',
    `actual_measure_number`                             int(11)             NOT NULL COMMENT '本次测量次数',
    `standard_measure_number`                           int(11)             NOT NULL COMMENT '本次达标测量次数',
    `systolic_blood_pressure_avg`                       decimal(5, 2)       NOT NULL COMMENT '本次收缩压平均值',
    `diastolic_blood_pressure_avg`                      decimal(5, 2)       NOT NULL COMMENT '本次舒张压平均值',
    `systolic_blood_pressure_standard_deviation`        decimal(5, 2)       NOT NULL COMMENT '本次收缩压标准差',
    `diastolic_blood_pressure_standard_deviation`       decimal(5, 2)       NOT NULL COMMENT '本次舒张压标准差',
    `is_blood_pressure_avg_standard`                    tinyint(1)          NOT NULL COMMENT '血压平均值是否达标',
    `actual_measure_number_deviation_value`             int(11)                      DEFAULT NULL COMMENT '本次测量(上传)次数与上次测量(上传)次数的差',
    `standard_measure_number_deviation_value`           int(11)                      DEFAULT NULL COMMENT '本次达标次数与上次达标次数的差',
    `systolic_blood_pressure_avg_deviation_value`       decimal(5, 2)                DEFAULT NULL COMMENT '本次收缩压平均值与上次收缩压平均值的差值',
    `diastolic_blood_pressure_avg_deviation_value`      decimal(5, 2)                DEFAULT NULL COMMENT '本次舒张压平均值与上次舒张压平均值的差值',
    `systolic_blood_pressure_standard_deviation_value`  decimal(5, 2)                DEFAULT NULL COMMENT '本次收缩压标准差与上次收缩压标准差的差值',
    `diastolic_blood_pressure_standard_deviation_value` decimal(5, 2)                DEFAULT NULL COMMENT '本次舒张压标准差与上次舒张压标准差的差值',
    `score_deviation_value`                             decimal(5, 2)                DEFAULT NULL COMMENT '本次报告得分与上次报告得分的差值',
    `created_by`                                        bigint(20) unsigned NOT NULL COMMENT '创建人',
    `created_at`                                        datetime            NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT '创建时间',
    PRIMARY KEY (`id`)
) ENGINE = InnoDB
  DEFAULT CHARSET = utf8mb4
  COLLATE = utf8mb4_general_ci COMMENT ='阶段性统计';

/* hs_stage_statistics_detail */
CREATE TABLE `hs_stage_statistics_detail`
(
    `id`                              bigint(20) unsigned NOT NULL,
    `stage_report_id`                 bigint(20) unsigned NOT NULL COMMENT '阶段性报告id',
    `systolic_blood_pressure`         decimal(5, 2)       NOT NULL COMMENT '收缩压',
    `diastolic_blood_pressure`        decimal(5, 2)       NOT NULL COMMENT '舒张压',
    `is_systolic_blood_pressure_max`  tinyint(1)          NOT NULL COMMENT '是否是收缩压最大值',
    `is_diastolic_blood_pressure_max` tinyint(1)          NOT NULL COMMENT '是否是舒张压最大值',
    `measure_datetime`                datetime            NOT NULL COMMENT '测量时间',
    `created_by`                      bigint(20) unsigned NOT NULL COMMENT '创建人',
    `created_at`                      datetime            NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT '创建时间',
    PRIMARY KEY (`id`)
) ENGINE = InnoDB
  DEFAULT CHARSET = utf8mb4
  COLLATE = utf8mb4_general_ci COMMENT ='阶段性统计明细';

