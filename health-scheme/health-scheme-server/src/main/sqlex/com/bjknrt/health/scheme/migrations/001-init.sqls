    `kn_patient_name` varchar(50)         NOT NULL COMMENT '患者名称',
    `kn_doctor_id`    bigint(20) unsigned NOT NULL COMMENT '医生ID',
    `kn_doctor_name`  varchar(50)         NOT NULL COMMENT '医生名称',
    `kn_status`       tinyint(1)          NOT NULL COMMENT '是否发送至患者端（0：否，1：是）',
    PRIMARY KEY (`kn_id`)
) ENGINE = InnoDB
  DEFAULT CHARSET = utf8mb4
  COLLATE = utf8mb4_general_ci COMMENT ='健康方案主表';

/* hs_health_scheme_management_info */
CREATE TABLE `hs_health_scheme_management_info`
(
    `kn_id`                 bigint(20) unsigned NOT NULL COMMENT '主键ID',
    `kn_management_stage`   varchar(255)                 DEFAULT NULL COMMENT '初始监测阶段:INITIAL_STAGE，短期稳定阶段:STABLE_STAGE，中期稳定阶段:METAPHASE_STABLE_STAGE，长期稳定阶段：SECULAR_STABLE_STAGE）',
    `kn_start_date`         date                         DEFAULT NULL COMMENT '阶段开始时间',
    `kn_end_date`           date                         DEFAULT NULL COMMENT '阶段结束时间',
    `kn_report_output_date` date                         DEFAULT NULL COMMENT '阶段性报告输出时间',
    `kn_created_by`         bigint(20) unsigned NOT NULL COMMENT '创建人',
    `kn_created_at`         datetime            NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT '创建时间',
    `is_del`                tinyint(1)          NOT NULL DEFAULT '0' COMMENT '删除状态 （1-是，0-否）',
    `kn_patient_id`         bigint(20) unsigned NOT NULL COMMENT '患者ID',
    `kn_job_id`             int(11)                      DEFAULT NULL COMMENT '任务id',
    `kn_health_manage_type` varchar(255)        NOT NULL COMMENT '健康方案类型 SYNTHETIC-综合健康方案，HYPERTENSION-高血压健康方案',
    `kn_disease_exists_tag` varchar(255)        NOT NULL COMMENT '健康方案病种患者标签,多个逗号隔开  HYPERTENSION-高血压 DIABETES-糖尿病 ACUTE_CORONARY_DISEASE-冠心病 CEREBRAL_STROKE-脑卒中 COPD-慢阻肺',
    PRIMARY KEY (`kn_id`)
) ENGINE = InnoDB
  DEFAULT CHARSET = utf8mb4
  COLLATE = utf8mb4_general_ci COMMENT ='健康方案管理表(个性化)';

/* hs_hsm_health_plan */
CREATE TABLE `hs_hsm_health_plan`
(
    `kn_id`                   bigint(20) unsigned NOT NULL COMMENT '主键ID',
    `kn_plan_type`            varchar(255)        NOT NULL COMMENT '计划类型（BLOOD_PRESSURE_MEASUREMENT: 测血压,SCIENCE_POPULARIZATION_PLAN: 科普计划,EXERCISE_PROGRAM: 运动方案,EXERCISE_PROGRAM_NOT_EVALUATED: 未进行评估(运动计划),EXERCISE_PROGRAM_ADJUSTMENT_REMIND: 运动调整提醒(运动计划),FASTING_BLOOD_GLUCOSE: 空腹血糖,MEAL_TWO_HOUR_RANDOM_BLOOD_GLUCOSE: 餐后2h/随机血糖,DIET_PLAN: 饮食计划,BEHAVIOR_VISIT: 行为习惯随访(线上随访),HYPERTENSION_VISIT: 高血压随访(线上随访),ONLINE_DIABETES: 糖尿病(线上随访),ONLINE_ACUTE_CORONARY_DISEASE: 冠心病(线上随访),ONLINE_CEREBRAL_STROKE: 脑卒中(线上随访),ONLINE_COPD: 慢阻肺(线上随访),OFFLINE_HYPERTENSION: 高血压(线下随访),OFFLINE_DIABETES: 糖尿病(线下随访),OFFLINE_ACUTE_CORONARY_DISEASE: 冠心病(线下随访),OFFLINE_CEREBRAL_STROKE: 脑卒中(线下随访),OFFLINE_COPD: 慢阻肺(线下随访)）',
    `kn_start_date`           datetime            NOT NULL COMMENT '开始时间',
    `kn_end_date`             datetime                     DEFAULT NULL COMMENT '结束时间',
    `kn_scheme_management_id` bigint(20) unsigned NOT NULL COMMENT '健康管理方案ID',
    `kn_foreign_plan_id`      bigint(20) unsigned NOT NULL COMMENT '健康计划ID',
    `is_del`                  tinyint(1)          NOT NULL DEFAULT '0' COMMENT '删除状态 （1-是，0-否）',
    `kn_job_id`               int(11)                      DEFAULT NULL COMMENT '任务id',
    PRIMARY KEY (`kn_id`)
) ENGINE = InnoDB
  DEFAULT CHARSET = utf8mb4
  COLLATE = utf8mb4_general_ci COMMENT ='健康计划表';

/* hs_htn_visit */
CREATE TABLE `hs_htn_visit`
(
    `id`                               bigint(20) unsigned NOT NULL,
    `patient_id`                       bigint(20) unsigned NOT NULL COMMENT '患者id',
    `patient_name`                     varchar(255)        NOT NULL COMMENT '患者姓名(手录)',
    `doctor_id`                        bigint(20) unsigned                    DEFAULT NULL COMMENT '随访医生id',
    `doctor_name`                      varchar(255)                           DEFAULT NULL COMMENT '随访医生姓名(手录)',
    `follow_way`                       varchar(16) COLLATE utf8mb4_general_ci DEFAULT NULL COMMENT '随访方式. SELF. 患者自填. OUTPATIENT. 门诊; FAMILY. 家庭; PHONE. 电话;ONLINE.线上',
    `follow_date`                      datetime                               DEFAULT NULL COMMENT '随访日期',
    `is_symptom_none`                  tinyint(1)                             DEFAULT NULL COMMENT '无症状',
    `is_symptom_dizziness_headache`    tinyint(1)                             DEFAULT NULL COMMENT '头痛头晕',
    `is_symptom_nausea_vomiting`       tinyint(1)                             DEFAULT NULL COMMENT '恶心呕吐',
    `is_symptom_blurred_tinnitus`      tinyint(1)                             DEFAULT NULL COMMENT '眼花耳鸣',
    `is_symptom_breathing`             tinyint(1)                             DEFAULT NULL COMMENT '呼吸困难',
    `is_symptom_palpitations_toc`      tinyint(1)                             DEFAULT NULL COMMENT '心悸胸闷',
    `is_symptom_nose_bleed`            tinyint(1)                             DEFAULT NULL COMMENT '鼻衄出血不止',
    `is_symptom_numbness`              tinyint(1)                             DEFAULT NULL COMMENT '四肢发麻',
    `is_symptom_lower_extremity_edema` tinyint(1)                             DEFAULT NULL COMMENT '下肢水肿',
    `symptom_other`                    varchar(255)                           DEFAULT NULL COMMENT '其它',
    `signs_sbp`                        double unsigned                        DEFAULT NULL COMMENT '血压 - 收缩压. 单位: mmHg',
    `signs_dbp`                        double unsigned                        DEFAULT NULL COMMENT '血压 - 舒张压. 单位: mmHg',
    `signs_height`                     double unsigned                        DEFAULT NULL COMMENT '身高. 单位: cm',
    `signs_weight`                     double unsigned                        DEFAULT NULL COMMENT '体重. 单位: kg',
    `signs_heart_rate`                 double unsigned                        DEFAULT NULL COMMENT '心率. 单位: 次/分钟',
    `signs_bim`                        double unsigned                        DEFAULT NULL COMMENT '体质指数. 单位: kg/m²',
    `signs_other`                      varchar(255)                           DEFAULT NULL COMMENT '体征 - 其他',
    `life_cigarettes_per_day`          tinyint(3) unsigned                    DEFAULT NULL COMMENT '日吸烟量. 单位: 支',
    `life_alcohol_per_day`             double unsigned                        DEFAULT NULL COMMENT '日饮酒量. 单位: 两',
    `life_sport_per_week`              tinyint(3) unsigned                    DEFAULT NULL COMMENT '每周运动次数. 单位: 次',
    `life_sport_per_time`              double unsigned                        DEFAULT NULL COMMENT '每次运动时间. 单位: 分钟',
    `life_salt_situation`              varchar(255)                           DEFAULT NULL COMMENT '摄盐情况. LIGHT: 轻; MIDDLE: 中; DEEP: 重',
    `life_mental_adjustment`           varchar(255)                           DEFAULT NULL COMMENT '心理调整. GOOD: 良好; ORDINARY: 一般; BAD: 差',
    `life_follow_medical_advice`       varchar(255)                           DEFAULT NULL COMMENT '遵医行为. GOOD: 良好; ORDINARY: 一般; BAD: 差',
    `abbreviations_about_examination`  varchar(255)                           DEFAULT NULL COMMENT '辅助检查',
    `drug_compliance`                  varchar(255)                           DEFAULT NULL COMMENT '服药依从性. LAW: 规律; GAP: 间断; NO: 不服药',
    `is_adr`                           tinyint(1)                             DEFAULT NULL COMMENT '有无药物不良反应',
    `adr_desc`                         varchar(255)                           DEFAULT NULL COMMENT '药物不良反应描述',
    `visit_class`                      varchar(255)                           DEFAULT NULL COMMENT '此次随访分类. SATISFACTORY: 控制满意; UNSATISFACTORY: 控制不满意; UNTOWARD_EFFECT: 不良反应; COMPLICATION: 并发症',
    `is_referral`                      tinyint(1)                             DEFAULT NULL COMMENT '是否转诊',
    `referral_reason`                  varchar(255)                           DEFAULT NULL COMMENT '转诊原因',
    `referral_agencies`                varchar(255)                           DEFAULT NULL COMMENT '转诊机构及科别',
    `next_visit`                       datetime                               DEFAULT NULL COMMENT '下次随访日期',
    `created_by`                       bigint(20) unsigned NOT NULL COMMENT '创建人',
    `created_at`                       timestamp           NOT NULL           DEFAULT CURRENT_TIMESTAMP COMMENT '创建时间',
    `updated_by`                       bigint(20) unsigned NOT NULL COMMENT '更新人',
    `updated_at`                       timestamp           NOT NULL           DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT '更新时间',
    `recommend_weight`                 double unsigned                        DEFAULT NULL COMMENT '建议体重. 单位: kg',
    `recommend_bim`                    double unsigned                        DEFAULT NULL COMMENT '建议体质指数',
    `recommend_cigarettes_per`         tinyint(3) unsigned                    DEFAULT NULL COMMENT '建议日吸烟量',
    `recommend_alcohol_per`            double unsigned                        DEFAULT NULL COMMENT '建议饮酒量',
    `recommend_sport_per_week`         tinyint(3) unsigned                    DEFAULT NULL COMMENT '建议每周运动次数 单位: 次',
    `recommend_sport_per_time`         double unsigned                        DEFAULT NULL COMMENT '建议每次运动时间. 单位: 分钟',
    `recommend_salt_situation`         varchar(16) COLLATE utf8mb4_general_ci DEFAULT NULL COMMENT '建议摄盐情况. LIGHT: 轻; MIDDLE: 中; DEEP: 重',
    PRIMARY KEY (`id`)
) ENGINE = InnoDB
  DEFAULT CHARSET = utf8mb4
  COLLATE = utf8mb4_general_ci COMMENT ='高血压患者随访表';

/* hs_htn_visit_drug */
